/* package codechef; // don't place package name! */

import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		String haystack = "hfdnhfehfeineedhhhhluei";
		String needle = "needle";
		System.out.println(strstr(haystack,needle));
	}
	static int strstr(final String haystack, final String needle){
	   String found ="";
	   int lookingTill = 0;
	   char h = 0;
	   char n = 0;
	    if (haystack.length()==0 || needle.length()==0)
	        return -1;
	    if (needle.length() > haystack.length())
	        return -1;
	    for(int j =0; j<haystack.length();j++) {
	         if (j==0) n = needle.charAt(j);
	         n = needle.charAt(lookingTill);
	         if(h==n){
	              
	              found += n;
	              lookingTill ++;
	         }
	         if (lookingTill >= needle.length()) break;
	        
	         h = haystack.charAt(j);
	        
	        
	       
	    }
	    System.out.println(found);
	    if (found.equals(needle)) return 1;
	    else return -1;
	}
	
}
