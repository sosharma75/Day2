public class Solution {
	public int maxProfit(final List<Integer> a) {
	    
	    int profit = 0;
	    Integer minElement = 0; //Integer.MAX_VALUE;
	    Integer maxElement = 0 ;//Integer.MAx_VALUE;
	    //System.out.println("minElement set to initially " + minElement);
	    if (a.size() <= 1) return 0;
	    for (int i =0; i <a.size(); i++){
	        minElement = Math.min(minElement, a.get(i));
	        maxElement = Math.max(maxElement, a.get(i));
	    }
	    return maxElement-minElement;
	    
	}
}
